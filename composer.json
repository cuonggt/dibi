{
    "name": "cuonggt/laravel-dibi",
    "description": "An elegant GUI database management tool for your Laravel applications.",
    "keywords": [
        "laravel",
        "database",
        "management"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Cuong Giang",
            "email": "thaicuong.giang@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "illuminate/contracts": "^5.8|^6.0|^7.0|^8.0",
        "illuminate/database": "^5.8|^6.0|^7.0|^8.0",
        "illuminate/support": "^5.8|^6.0|^7.0|^8.0",
        "ramsey/uuid": "^3.8|^4.0"
    },
    "require-dev": {
        "orchestra/database": "^3.8|^4.0|^5.0|^6.0",
        "orchestra/testbench": "^3.8|^4.0|^5.0|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Cuonggt\\Dibi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cuonggt\\Dibi\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cuonggt\\Dibi\\DibiServiceProvider"
            ],
            "aliases": {
                "Dibi": "Cuonggt\\Dibi\\Dibi"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
