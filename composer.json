{
    "name": "cuonggt/dibi",
    "description": "Fast, easy-to-use Laravel database management application for working with MySQL databases.",
    "keywords": [
        "laravel",
        "database",
        "management"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Giang Thai Cuong",
            "email": "thaicuong.giang@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "illuminate/contracts": "~5.5",
        "illuminate/support": "~5.5",
        "illuminate/database": "~5.5"
    },
    "require-dev": {
        "mockery/mockery": "~1.0",
        "orchestra/database": "~3.5",
        "orchestra/testbench": "~3.5",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Cuonggt\\Dibi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cuonggt\\Dibi\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cuonggt\\Dibi\\DibiServiceProvider"
            ],
            "aliases": {
                "Dibi": "Cuonggt\\Dibi\\Dibi"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
